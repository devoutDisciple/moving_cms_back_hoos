{
    "presets": [
        ["@babel/preset-env", {
            "targets": {
                "edge": "17",
                "firefox": "60",
                "chrome": "67",
                "safari": "11.1",
                "browsers": "> 0.25%"
            },
            "useBuiltIns": "usage"
        }],
        "@babel/preset-react"
    ],
    "plugins": [
        ["import",
            {
                "libraryName": "antd",
                "style": "css"
            }
        ]
    ]
}